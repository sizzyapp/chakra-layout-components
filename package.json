{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": "https://github.com/sizzyapp/chakra-layout-components",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "chakra-layout-components",
  "author": "Kitze",
  "module": "dist/chakra-layout-components.esm.js",
  "size-limit": [
    {
      "path": "dist/chakra-layout-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chakra-layout-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@chakra-ui/react": "^1.3.4",
    "@emotion/styled": "^11.1.5",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "framer-motion": "^4.0.0",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.1",
    "styled-mixins": "^0.1.10",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.3.4",
    "@emotion/styled": "^11.1.5",
    "framer-motion": "^4.0.0",
    "react": ">=16",
    "styled-mixins": "^0.1.10"
  }
